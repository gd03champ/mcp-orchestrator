{
  "mcpServers": {
    "aws-documentation-mcp-server": {
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "--interactive",
        "--env",
        "FASTMCP_LOG_LEVEL=ERROR",
        "--env",
        "AWS_DOCUMENTATION_PARTITION=aws",
        "mcp/aws-documentation:latest"
      ],
      "env": {
        "AWS_REGION": "us-west-2"
      },
      "disabled": false,
      "autoApprove": []
    },
    "github-mcp-server": {
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "--interactive",
        "mcp/github-api:latest"
      ],
      "env": {},
      "disabled": true,
      "autoApprove": []
    }
  }
}
